	<rdfs:Class rdf:about="http://www.tracker-project.org/temp/slo#GeoLocation">
		<rdfs:comment>A place in the space, can be defined by coordinates or text</rdfs:comment>
		<rdfs:label>Location</rdfs:label>
		<tracker:added>2010-11-30T13:24:06Z</tracker:added>
		<tracker:modified>2086</tracker:modified>
		<rdfs:subClassOf rdf:resource="http://www.semanticdesktop.org/ontologies/2007/01/19/nie#InformationElement"/>
	</rdfs:Class>
	<rdf:Property rdf:about="http://www.tracker-project.org/temp/slo#latitude">
		<rdfs:comment>Positive values for the north hemisphere, negative for the south</rdfs:comment>
		<rdfs:label>Latitude</rdfs:label>
		<tracker:added>2010-11-30T13:24:06Z</tracker:added>
		<tracker:modified>2059</tracker:modified>
		<rdfs:domain rdf:resource="http://www.tracker-project.org/temp/slo#GeoLocation"/>
