		<tracker:modified>2073</tracker:modified>
		<rdfs:domain rdf:resource="http://www.tracker-project.org/temp/slo#GeoLocation"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
	</rdf:Property>
	<rdf:Property rdf:about="http://www.tracker-project.org/temp/slo#verticalAccuracy">
		<rdfs:comment>Represents the error estimate of vertical (earths-surface) accuracy in meters.</rdfs:comment>
		<rdfs:label>Latitude</rdfs:label>
		<tracker:added>2010-11-30T13:24:06Z</tracker:added>
		<tracker:modified>2074</tracker:modified>
